# WireGuard/LAN bind IP for host port 9150 (e.g., 10.10.1.1). Leave empty to bind all.
LAN_BIND_IP=

# Allowed client subnet (CIDR). Clients outside this will be rejected at proxy.
LAN_CIDR=10.10.1.0/24

# Allow proxying to private/reserved destination IPs (0 = block, 1 = allow)
ALLOW_PRIVATE_DEST=0
